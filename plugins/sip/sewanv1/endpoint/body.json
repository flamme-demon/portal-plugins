{
    "label": "{{ label }}",
    "name": "sewan_trunk_{{ tenant_uuid }}_{{ suffix }}",
    "transport": {"uuid": "{{ transport_uuid }}"},
    "templates": [{"uuid": "{{ global_sip_template_uuid }}"}, {"uuid":"{{ registration_trunk_sip_template_uuid }}"}],
    "endpoint_section_options": [
        ["identify_by", "header,username"],
        ["from_domain", "{{ from_domain }}"],
        ["send_pai", "yes"],
        ["allow", "!all,alaw"]
    ],
    "registration_section_options": [
        ["expiration", "600"],
        ["line", "yes"],
        ["client_uri", "sip:{{ username }}@{{ domain }}:5070"],
        ["server_uri", "sip:{{ domain }}:5070"],
        ["outbound_proxy", "sip:{{ server }}:5070\\;lr"]
    ],
    "registration_outbound_auth_section_options": [
        ["username", "{{ username }}"],
        ["password", "{{ password }}"]
    ],
    "outbound_auth_section_options": [
        ["username", "{{ username }}"],
        ["password", "{{ password }}"]
    ],
    "aor_section_options": [
        ["contact", "sip:{{ username }}@{{ server }}:5060"]
    ],
    "identify_section_options": [
        ["match_header", "Contact: /<sip:{{ username }}@.*>/"],
        ["match", "{{ server }}"]
    ]
}
