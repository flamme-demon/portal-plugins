{
    "label": "{{ label }}",
    "name": "keyyo_trunk_{{ tenant_uuid }}_{{ suffix }}",
    "transport": {"uuid": "{{ transport_uuid }}"},
    "templates": [{"uuid": "{{ global_sip_template_uuid }}"}, {"uuid":"{{ registration_trunk_sip_template_uuid }}"}],
    "endpoint_section_options": [
        ["identify_by", "auth_username,username,ip"],
        ["allow", "!all,alaw,g729,g722"],
        ["send_pai", "yes"],
        ["from_user", "{{ username }}"]
    ],
    "registration_section_options": [
        ["contact_user", "{{ username }}"],
        ["expiration", "120"],
        ["line", "yes"],
        ["client_uri", "sip:{{ username }}@keyyo.net:5060"],
        ["server_uri", "sip:{{ username }}@keyyo.net:5060"]
    ],
    "registration_outbound_auth_section_options": [
        ["username", "{{ username }}"],
        ["password", "{{ password }}"]
    ],
    "outbound_auth_section_options": [
        ["username", "{{ username }}"],
        ["password", "{{ password }}"]
    ],
    "aor_section_options": [
        ["contact", "sip:{{ username }}@keyyo.net:5060"]
    ],
    "identify_section_options": [
        ["match", "keyyo.net"]
    ]
}
